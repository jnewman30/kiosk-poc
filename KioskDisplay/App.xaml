														</StackPanel>
													</Button>
													<Button Style="{StaticResource InformButton}" VerticalContentAlignment="Top" Command="controls:TouchScreenKeyboard.CmdH"  HorizontalContentAlignment="Left"  Width="50" Height="50">
														<StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" >
															<TextBlock    HorizontalAlignment="Left" VerticalAlignment="Top"   >H</TextBlock>
															<TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom"   ></TextBlock>
														</StackPanel>
													</Button>
													<Button Style="{StaticResource InformButton}" VerticalContentAlignment="Top" Command="controls:TouchScreenKeyboard.CmdJ"  HorizontalContentAlignment="Left"  Width="50" Height="50">
