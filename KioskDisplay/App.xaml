															<TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom"   ></TextBlock>
														</StackPanel>
													</Button>
													<Button Style="{StaticResource InformButton}" VerticalContentAlignment="Top" Command="controls:TouchScreenKeyboard.CmdA"  HorizontalContentAlignment="Left"  Width="50" Height="50">
														<StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" >
