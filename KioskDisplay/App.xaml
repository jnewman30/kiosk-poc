															<TextBlock    HorizontalAlignment="Left" VerticalAlignment="Top"   >!</TextBlock>
															<TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom"   >1</TextBlock>
														</StackPanel>
													</Button>
													<Button Style="{StaticResource InformButton}" Command="controls:TouchScreenKeyboard.Cmd2"  VerticalContentAlignment="Top" HorizontalContentAlignment="Left"  Width="50" Height="50">
