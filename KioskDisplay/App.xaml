															<TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom"   ></TextBlock>
														</StackPanel>
													</Button>
													<Button Style="{StaticResource InformButton}" VerticalContentAlignment="Top" Command="controls:TouchScreenKeyboard.CmdG"  HorizontalContentAlignment="Left"  Width="50" Height="50">
