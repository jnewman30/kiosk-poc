													<Button Style="{StaticResource InformButton}" Command="controls:TouchScreenKeyboard.CmdQ" VerticalContentAlignment="Top" HorizontalContentAlignment="Left"  Width="50" Height="50">
														<StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" >
