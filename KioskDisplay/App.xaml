															<TextBlock    HorizontalAlignment="Left" VerticalAlignment="Top"   >~</TextBlock>
															<TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom"   >`</TextBlock>
														</StackPanel>
													</Button>
													<Button  Command="controls:TouchScreenKeyboard.Cmd1" Style="{StaticResource InformButton}" VerticalContentAlignment="Top" HorizontalContentAlignment="Left"  Width="50" Height="50">
